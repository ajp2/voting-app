<% include ./header %>

<div class="container">
  <br>
  <h1>Polls</h1>
  <p>Select a poll below to see the results and vote</p>
  <hr>
  <br>
  <ul>
    <% allPolls.forEach(poll => { %>
    <li>
      <a href="/polls/<%= poll.id %>">
        <%= poll.title %></a>
    </li>
    <% }) %>
  </ul>
</div>

<% include ./footer %>